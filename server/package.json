{
  "name": "api",
  "version": "0.0.1",
  "description": "Algorand sdk example api",
  "author": "Kevin Grassi",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "deploy": "cd .. && git subtree push --prefix server heroku-server master"
  },
  "dependencies": {
    "algosdk": "^1.9.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^4.0.4",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^12.0.6",
    "nodemon": "^1.19.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.5.3",
    "ts-node": "^8.2.0",
    "tslint": "5.20.1"
  }
}
